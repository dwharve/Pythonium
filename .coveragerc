[run]
source = pythonium/mcp
omit =
    pythonium/mcp/server.py
    pythonium/mcp/handlers/*
    pythonium/mcp/formatters/*
    pythonium/mcp/services/*
    pythonium/mcp/storage/*
    pythonium/mcp/core/*
    pythonium/mcp/utils/*
    pythonium/mcp/tools/__init__.py
    pythonium/mcp/tools/configuration.py

[report]
show_missing = True
